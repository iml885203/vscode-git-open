{
  "name": "git-open",
  "displayName": "Git Open",
  "description": "helps you quickly open the remote repository of the current project",
  "version": "2025.4.2",
  "publisher": "loganguo",
  "author": {
    "name": "Logan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logan/vscode-git-open"
  },
  "bugs": {
    "url": "https://github.com/logan/vscode-git-open/issues"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "github",
    "gitlab",
    "bitbucket",
    "azure-devops",
    "repository",
    "pull-request",
    "merge-request",
    "pipeline",
    "ci-cd"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-open.openRemoteRepo",
        "title": "Git Open: Open Remote Repository"
      },
      {
        "command": "git-open.openMergeRequests",
        "title": "Git Open: Open Merge Requests/Pull Requests"
      },
      {
        "command": "git-open.createMergeRequest",
        "title": "Git Open: Create Merge Request/Pull Request"
      },
      {
        "command": "git-open.openPipelines",
        "title": "Git Open: Open Pipelines/Actions"
      }
    ],
    "keybindings": [
      {
        "command": "workbench.action.quickOpen",
        "args": ">Git Open",
        "key": "alt+g alt+g",
        "mac": "alt+g alt+g"
      },
      {
        "command": "git-open.openRemoteRepo",
        "key": "alt+g alt+o",
        "mac": "alt+g alt+o"
      },
      {
        "command": "git-open.openMergeRequests",
        "key": "alt+g alt+m",
        "mac": "alt+g alt+m"
      },
      {
        "command": "git-open.createMergeRequest",
        "key": "alt+g alt+r",
        "mac": "alt+g alt+r"
      },
      {
        "command": "git-open.openPipelines",
        "key": "alt+g alt+p",
        "mac": "alt+g alt+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "glob": "^11.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2"
  }
}
