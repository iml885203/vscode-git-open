{
  "name": "git-open",
  "displayName": "Git Open",
  "description": "Quickly open Git repositories, PRs, and CI/CD pipelines. Supports GitHub, GitLab, Bitbucket, and Azure DevOps.",
  "version": "2026.2.1",
  "publisher": "loganguo",
  "author": {
    "name": "Logan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iml885203/vscode-git-open"
  },
  "bugs": {
    "url": "https://github.com/iml885203/vscode-git-open/issues"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "github",
    "gitlab",
    "bitbucket",
    "azure-devops",
    "repository",
    "pull-request",
    "merge-request",
    "pipeline",
    "ci-cd"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-open.openRemoteRepo",
        "title": "Git Open: Open Remote Repository"
      },
      {
        "command": "git-open.openMergeRequests",
        "title": "Git Open: Open Merge Requests/Pull Requests"
      },
      {
        "command": "git-open.createMergeRequest",
        "title": "Git Open: Create Merge Request/Pull Request"
      },
      {
        "command": "git-open.openPipelines",
        "title": "Git Open: Open Pipelines/Actions"
      },
      {
        "command": "git-open.showQuickPick",
        "title": "Git Open: Show Quick Pick Menu"
      }
    ],
    "keybindings": [
      {
        "command": "git-open.showQuickPick",
        "key": "alt+g alt+g",
        "mac": "alt+g alt+g"
      },
      {
        "command": "git-open.openRemoteRepo",
        "key": "alt+g alt+o",
        "mac": "alt+g alt+o"
      },
      {
        "command": "git-open.openMergeRequests",
        "key": "alt+g alt+m",
        "mac": "alt+g alt+m"
      },
      {
        "command": "git-open.createMergeRequest",
        "key": "alt+g alt+r",
        "mac": "alt+g alt+r"
      },
      {
        "command": "git-open.openPipelines",
        "key": "alt+g alt+p",
        "mac": "alt+g alt+p"
      }
    ],
    "configuration": {
      "title": "Git Open",
      "properties": {
        "git-open.providerDomains": {
          "type": "object",
          "description": "Custom domain mappings for Git providers. Key is the domain, value is the provider type ('github', 'gitlab', 'bitbucket', 'azure').",
          "additionalProperties": {
            "type": "string",
            "enum": [
              "github",
              "gitlab",
              "bitbucket",
              "azure"
            ]
          },
          "default": {
            "github.com": "github",
            "gitlab.com": "gitlab",
            "bitbucket.org": "bitbucket",
            "visualstudio.com": "azure",
            "dev.azure.com": "azure"
          },
          "examples": [
            {
              "git.company.com": "gitlab",
              "custom-github.org": "github"
            }
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:publish": "vsce publish --no-dependencies",
    "ovsx:publish": "ovsx publish --no-dependencies",
    "publish": "pnpm run vscode:publish && pnpm run ovsx:publish",
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.2",
    "glob": "^11.0.1",
    "npm-run-all2": "^8.0.4",
    "ovsx": "^0.10.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
